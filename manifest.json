{
	"manifest_version": 3,
	"name": "Remove theme-color",
	"description": "Remove <meta name=theme-color> from all websites",
	"version": "13.1.2023",
	"icons": {
		"128": "icon 128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"contentscripts.js"
			],
			"run_at": "document_start"
		}
	]
}